alias sl=ls
alias l=ls
alias ls='ls --color'
alias la='ls -a'
alias ll='ls -l'
alias cp='cp -i'
alias mv='mv -i'
alias p=less
alias r=rm

alias service='rc-service'
alias tcat='tar tvf'
alias info='info --vi-keys'
alias clip='xclip -selection clipboard'
alias lynx='lynx -vikeys'
alias show-keysym='xev 2>&1 | grep -F keysym'
alias gdb='gdb -q'
